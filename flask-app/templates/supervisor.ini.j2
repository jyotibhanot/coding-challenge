[program:{{ app_name }}]
command=/usr/local/bin/uwsgi
  --socket /tmp/{{ app_name }}.sock
  --logto {{ webapps_dir }}/{{ app_name }}/log/application.log
  --uid {{ flaskapp_user }}
  --home {{ webapps_dir }}/venv
  --pythonpath {{ webapps_dir }}
  --wsgi-file {{ webapps_dir }}/{{ wsgi_file }}
  --callable {{ wsgi_callable }}
  --max-requests 1000
  --master
  --processes 1
  --chmod
user=www-data
directory={{ webapps_dir }}
autostart=true
autorestart=true
